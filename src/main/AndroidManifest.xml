<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.accountinfosaver">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.AboutUsActivity"></activity>
        <activity android:name=".activities.SetPasswordActivity" />
        <activity android:name=".activities.PasswordActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.StartingActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.SettingsActivity" />
        <activity android:name=".activities.PasswordGeneratorActivity" />
        <activity android:name=".activities.EditAccountActivity" />
        <activity android:name=".activities.AccountInfoActivity" />
        <activity android:name=".activities.CreateAccountInfoActivity" />
        <activity android:name=".activities.MainActivity"></activity>
    </application>

</manifest>